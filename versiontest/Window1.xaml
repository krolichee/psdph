<Window x:Class="versiontest.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:versiontest"
        mc:Ignorable="d"
        Title="Window1" Height="360" Width="175">
    <Grid>
        <Frame Content="Frame" Margin="0,148,0,0" BorderBrush="#FF15B31F" Background="#FFE60E0E" Foreground="#FF84D824" HorizontalAlignment="Left" VerticalAlignment="Top" Height="196" Width="175">
            <Frame.Effect>
                <DropShadowEffect Direction="94" ShadowDepth="14" BlurRadius="45" Opacity="0.595"/>
            </Frame.Effect>
        </Frame>
        <Button Content="Button" HorizontalAlignment="Left" Margin="48,214,0,0" VerticalAlignment="Top"/>
        <StackPanel>
            <StackPanel Height="119">
                <Button Content="Button"/>
                <Button Content="Button" Margin="0,1,0,0"/>
                <UniformGrid Columns="3">
                    <Button Content="Button 1"/>
                    <Button Content="Button 2" Height="20"/>
                    <Button Content="Button 3" Height="30"/>
                    <Button Content="Button 4" Height="20"/>
                    <Button Content="Button 5" Height="20"/>
                </UniformGrid>
                <TreeView Height="100">
                    <TreeViewItem Header="dsad" IsExpanded="True">
                        <Menu/>

                    </TreeViewItem>
                </TreeView>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
