<Window x:Class="psdPH.CollectionEditor.CollectionEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:psdPH.CollectionEditor"
        mc:Ignorable="d"
        Title="CollectionEditor" Height="450" Width="800">
    <Grid>
        <ScrollViewer HorizontalAlignment="Center" Height="110" Margin="0,324,0,0" VerticalAlignment="Top" Width="800" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden">
            <StackPanel x:Name="imagesStackPanel" Orientation="Horizontal"/>
        </ScrollViewer>
        <Label Content="Коллекция:" HorizontalAlignment="Left" Margin="10,25,0,0" VerticalAlignment="Top"/>
        <Menu VerticalAlignment="Top" Height="20">
            <MenuItem x:Name="collectionsMenuItem" Header="Коллекции"></MenuItem>
        </Menu>
        <Label x:Name="collectionNameLabel" Content="CollectionName" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" FontWeight="Bold"/>

    </Grid>
</Window>
