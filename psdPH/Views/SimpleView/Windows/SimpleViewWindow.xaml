<Window x:Class="psdPH.Views.SimpleView.Windows.SimpleViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:psdPH.Views.SimpleView.Windows" xmlns:CedStack="clr-namespace:psdPH.Utils.CedStack"
        mc:Ignorable="d"
        Title="SimpleViewWindow" Height="450" Width="800">
    <Grid x:Name="mainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="18"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid x:Name="cedStackGrid" Grid.Row="1">
            <CedStack:CEDStackUI Margin="0,0,0,0"/>
        </Grid>
        <Menu VerticalAlignment="Center" Height="18">
            <MenuItem Header="Вид" >
                <MenuItem Header="Удалить" Click="MenuItem_Click" />
            </MenuItem>
        </Menu>
    </Grid>
</Window>
