<Window x:Class="psdPH.CompositionLeafEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:psdPH"
        mc:Ignorable="d"
        Title="EditCompositionWindow" Height="140" Width="162">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Ok" HorizontalAlignment="Center" Margin="0,82,0,0" VerticalAlignment="Top" Width="49" Click="Button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="0,51,0,0"/>

    </Grid>
</Window>
