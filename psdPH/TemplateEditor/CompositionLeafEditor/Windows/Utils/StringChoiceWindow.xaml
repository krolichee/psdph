<Window x:Class="psdPH.StringChoiceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:psdPH"
        mc:Ignorable="d"
        Title="Выбор строки" Height="140" Width="225">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>


        <StackPanel x:Name="stackPanel" Grid.ColumnSpan="2">
            <Button Content="Ok" HorizontalAlignment="Center" VerticalAlignment="Top" Width="49" Click="Button_Click" Margin="0,20,0,0"/>
        </StackPanel>



    </Grid>
</Window>
