<Window x:Class="psdPH.TemplateEditor.CompositionLeafEditor.Windows.Utils.SetupsInputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:psdPH.TemplateEditor.CompositionLeafEditor.Windows.Utils"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="StackOkWindow"  MinWidth="200" MinHeight="50" SizeToContent="WidthAndHeight" Closed="Window_Closed" ResizeMode="NoResize">
    <StackPanel x:Name="MainGrid">
        <Button Content="Ok" HorizontalAlignment="Center"  VerticalAlignment="Top" Height="25" Width="40" Margin="0,10,0,10" Click="Button_Click"/>
    </StackPanel>
</Window>
