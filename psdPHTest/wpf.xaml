<UserControl x:Class="psdPHTest.wpf"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:psdPHTest"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ComboBox HorizontalAlignment="Left" Margin="380,190,0,0" VerticalAlignment="Top" Width="120" IsEditable="True"/>
        <Expander HorizontalAlignment="Left" Height="100" Header="Expander" Margin="225,201,0,0" VerticalAlignment="Top" Width="100">
            <Grid Background="#FFE5E5E5"/>
        </Expander>
        <Frame Content="Frame" Foreground="#FFCC1E1E" Background="#FFAF1515"/>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0,0,495,290"/>
        <Rectangle HorizontalAlignment="Left" Height="100" Margin="380,270,0,0" VerticalAlignment="Top" Width="60">
            <Rectangle.Fill>
                <VisualBrush Viewport="6,6,15,15" ViewportUnits="Absolute" TileMode="Tile" Viewbox="0,0,3,3">
                    <VisualBrush.Visual>
                        <Canvas Width="15" Height="15">
                            <Ellipse Width="5" Height="5" Fill="#FF87888A" 
                             Canvas.Left="5" Canvas.Top="6" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        </Canvas>
                    </VisualBrush.Visual>
                </VisualBrush>
            </Rectangle.Fill>
        </Rectangle>
        <ProgressBar HorizontalAlignment="Left" Height="10" Margin="440,185,0,0" VerticalAlignment="Top" Width="100" IsIndeterminate="True"/>

    </Grid>
</UserControl>
